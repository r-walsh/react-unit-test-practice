{
  "name": "react-unit-test-practice",
  "version": "1.0.0",
  "description": "A repository to practice your react unit testing skills",
  "main": "./src/index.js",
  "repository": "https://github.com/r-walsh/react-unit-test-practice",
  "keywords": [
    "react",
    "unit-testing",
    "enzyme",
    "jest",
    "utahjs",
    "midwestjs"
  ],
  "author": "Ryan Walsh <imryanwalsh@gmail.com> (https://twitter.com/_rtwalsh)",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dashboard -- webpack-dev-server --hot --inline --progress",
    "prettier": "prettier --single-quote --trailing-comma all --write \"./src/**/*.js\" \"./webpack.config.js\" \"./test-config/*.js\"",
    "test": "cross-env NODE_ENV=test node ./test-config",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "view-coverage": "live-server ./coverage"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|png|svg)$": "<rootDir>/test-config/file-mock.js",
      "^.+\\.styl": "<rootDir>/test-config/style-mock.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/test-config/transform.js"
    },
    "setupFiles": [
      "<rootDir>/test-config/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(dist|node_modules)/"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "\\.test\\.js$",
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "jest": "^20.0.4",
    "jsdom": "^11.1.0",
    "live-server": "^1.2.0",
    "prettier": "^1.5.3",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.5.2",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.7.1"
  }
}
